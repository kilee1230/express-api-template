{
  "name": "express-api-template",
  "version": "1.0.0",
  "description": "Boilerplate for express api",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "rimraf ./dist && node esbuild.config.js",
    "build:tsc": "rimraf ./dist && tsc -p tsconfig.build.json",
    "lint": "eslint --config eslint.config.js",
    "lint:fix": "pnpm lint --fix",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development nodemon --watch src --ext ts --exec \"npx tsx src/server.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "2.2.0",
    "cors": "2.8.5",
    "express": "5.1.0",
    "pino-http": "10.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/types": "^30.0.5",
    "@types/body-parser": "1.19.6",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.6.2",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "esbuild": "^0.25.8",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "30.0.5",
    "nodemon": "3.1.10",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "tsx": "^4.20.3",
    "typescript": "5.9.3"
  }
}
